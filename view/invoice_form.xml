<data>
    <xpath expr="/form/notebook/page[@id='invoice']/field[@name='lines']"
            position="before">
        <label name="conformity_state"/>
        <field name="conformity_state"/>
    </xpath>
    <xpath expr="/form/notebook/page[@id='info']" position="after">
        <page string="Review Conformity" id="conform" col="6">
            <label name="conformity_state"/>
            <field name="conformity_state"/>
            <label name="conform_by"/>
            <field name="conform_by"/>
            <label name="nonconformity_culprit"/>
            <field name="nonconformity_culprit"/>
            <separator name="conforming_description" colspan="6"/>
            <field name="conforming_description" colspan="6"/>
        </page>
    </xpath>
</data>
